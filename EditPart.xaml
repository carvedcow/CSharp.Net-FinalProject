<Window x:Class="CSharp.Net_FinalProject.EditPart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharp.Net_FinalProject"
        mc:Ignorable="d"
        Title="Edit Part" Height="250" Width="300">
    <StackPanel Margin="10">
        <Label Content="Type"></Label>
        <ComboBox Width="120" HorizontalAlignment="Left">
            <ComboBoxItem>1</ComboBoxItem>
            <ComboBoxItem>2</ComboBoxItem>
            <ComboBoxItem>3</ComboBoxItem>
            <ComboBoxItem>4</ComboBoxItem>
            <ComboBoxItem>5</ComboBoxItem>
        </ComboBox>
        <Label Content="Brand"></Label>
        <TextBox Width="120" HorizontalAlignment="Left"></TextBox>
        <Label Content="Series"></Label>
        <TextBox Width="120" HorizontalAlignment="Left"></TextBox>
        <Label Content="Name"></Label>
        <TextBox Width="120" HorizontalAlignment="Left"></TextBox>
        
    </StackPanel>
</Window>
